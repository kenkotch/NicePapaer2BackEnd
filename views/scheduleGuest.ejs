<h4 class="text-center"><%= title %></h4>

<!-- schedule -->
<div  class="center-content">

  <% if (wedDate) { %>
    <h3><%= wedDate %></h3>
  <% } %>

  <table id="schedule">
    <tr>
      <th>time</th>
      <th>event</th>
      <th>description</th>
    </tr>

    <% for (let i = 0; i < data.length; i++) { %>
      <% if(data[i].time !== 'hide') { %>
      <tr>
        <td><%= data[i].time %></td>
        <td><%= data[i].item %></td>
        <td><%= data[i].description %></td>
      </tr>
      <% } %>
    <% } %>
  </table>

</div>
